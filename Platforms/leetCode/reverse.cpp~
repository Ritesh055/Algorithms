#include <iostream>
#include <math.h>

using namespace std;

int reverse(int n) {
  bool neg;
  (n > 0) ? neg = true : neg = false;
  int m = n;
  int count=0;

  while (n > 0) {
    count++;
    n/=10;
  }

  int temp=0;
  int ind=0;

  while (count--) {
    temp += (n%10) * pow(10,ind);
    ind++;
    n /= 10;
  }

  (neg) ? temp = -(temp) : temp = temp;
  
  return temp;
}


int main() {
  int n;

  cin >> n;

  

}
