#include <iostream>
using namespace std;

int minTrack(int n,int AT[]) {
  int k,ind;
  int min=AT[0];
  for (k=0;k<n;k++) {
    if (AT[k] < min) min = AT[k]; ind = k;
  }
  return ind;
}


